<div class="efb-wrap-list efb-wrap-list-forms">
    <div class='efb-list-buttons text-right'>
        {{#pageactions}}
            {{< local_edwiserform/header-action-buttons}}
                    {{$ button-class }}efb-list-button{{/ button-class }}
                {{/ local_edwiserform/header-action-buttons}}
        {{/pageactions}}
    </div>
    <hr>
    <h2>{{heading}}</h2>
    <table id="efb-forms" class="efb-table efb-forms-table w-100">
        <thead>
            <tr>
                {{#headings}}
                    <th>{{{.}}}</th>
                {{/headings}}
            </tr>
        </thead>
    </table>
    <div class="efb-modal" id="efb-modal" data-region="modal-container" aria-hidden="false" role="dialog">
        <div class="efb-modal-dialog " role="document" data-region="efb-modal" aria-labelledby="17-modal-title">
            <div class="efb-modal-content">
                <div class="efb-modal-header" data-region="header">
                    <h4 id="efb-modal-title" class="efb-modal-title text-white" data-region="title" tabindex="0"></h4>
                    <button type="button" class="close efb-modal-close text-white" data-action="hide" aria-label="Close">
                      <span aria-hidden="true">Ã—</span>
                    </button>
                </div>
                <div class="efb-modal-body" data-region="body">
                </div>
                <div class="efb-modal-footer" data-region="footer">
                    <button class="btn btn-danger efb-modal-delete-form" type="button">{{#str}} proceed, local_edwiserform {{/str}}</button>
                    <button class="btn btn-success efb-modal-pro-activate" type="button">{{#str}} activatelicense, local_edwiserform {{/str}}</button>
                    <button class="btn btn-success efb-modal-close efb-modal-delete-close" type="button">{{#str}} cancel, local_edwiserform {{/str}}</button>
                    <button class="btn btn-warning efb-modal-close efb-modal-pro-activate-close" type="button">{{#str}} cancel, local_edwiserform {{/str}}</button>
                </div>
            </div>
        </div>
    </div>
</div>
