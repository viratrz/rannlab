<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/cocoon_form_builder/db" VERSION="20201026" COMMENT="Cocoon Form Builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="cocoon_form_builder" COMMENT="Cocoon Form Builder">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="cocoon_form_builder_forms" COMMENT="Cocoon Form Builder">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="title" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Cocoon Form Builder"/>
        <FIELD NAME="json" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Cocoon Form Builder"/>
        <FIELD NAME="data" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Cocoon Form Builder"/>
        <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Cocoon Form Builder"/>
        <FIELD NAME="confirm_message" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Cocoon Form Builder"/>
        <FIELD NAME="recipients" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Cocoon Form Builder"/>
        <FIELD NAME="status" TYPE="int" LENGTH="1" NOTNULL="false" DEFAULT="1" SEQUENCE="false" COMMENT="Cocoon Form Builder"/>
        <FIELD NAME="ajax" TYPE="int" LENGTH="1" NOTNULL="false" DEFAULT="1" SEQUENCE="false" COMMENT="Cocoon Form Builder"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
