{{#webhost}}
    <p>{{#str}} webhost, local_edusupport {{/str}}: {{webhost}}</a></p>
{{/webhost}}
{{#url}}
    <p>{{#str}} weburl, local_edusupport {{/str}}: <a href="{{url}}" target="_blank">{{url}}</a></p>
{{/url}}
{{#contactphone}}
    <p><a href="tel:{{contactphone}}">{{contactphone}}</a></p>
{{/contactphone}}
{{#includeemail}}
    <p><a href="mailto:{{includeemail}}">{{includeemail}}</a></p>
{{/includeemail}}
<div>
    {{{description}}}
</div>
