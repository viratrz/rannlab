{{{custom_css_dashboard}}}
<header class="header-nav menu_style_home_one dashbord_pages navbar-scrolltofixed stricky main-menu {{#disable_stricky_dashboard}} ccn_disable_stricky {{/disable_stricky_dashboard}}">
  <div class="container-fluid">
    <nav>
      <div class="menu-toggle">
        <img class="nav_logo_img img-fluid" src="{{{headerlogo1}}}" alt="{{{sitename}}}">
        <button type="button" id="menu-btn">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      </div>
      <a href="{{#in_course_activity}}{{{ccn_course_url}}}{{/in_course_activity}}{{^in_course_activity}}{{{ccn_dashboard_url}}}{{/in_course_activity}}" class="btn btn-primary ccn-btn-backtocourse float-left"><span class="flaticon-left-arrow"></span> {{#in_course_activity}}{{#str}} back_to_course, theme_edumy {{/str}}{{/in_course_activity}}{{^in_course_activity}}{{#str}} back_to_site, theme_edumy {{/str}}{{/in_course_activity}}</a>
      <h4 class="title float-left">{{{ pageheading }}}</h4>
      {{> theme_edumy/ccn_navbar_user }}
    </nav>
  </div>
</header>
<div id="page" class="stylehome1 h0">
  <div class="mobile-menu">
    <div class="header stylehome1 dashbord_mobile_logo dashbord_pages">
      <a href="{{#in_course_activity}}{{{ccn_course_url}}}{{/in_course_activity}}{{^in_course_activity}}{{{ccn_dashboard_url}}}{{/in_course_activity}}" class="btn btn-primary ccn-btn-backtocourse float-left"><span class="flaticon-left-arrow"></span> {{#in_course_activity}}{{#str}} back_to_course, theme_edumy {{/str}}{{/in_course_activity}}{{^in_course_activity}}{{#str}} back_to_site, theme_edumy {{/str}}{{/in_course_activity}}</a>
      <ul class="menu_bar_home2">
        <li class="list-inline-item"></li>
        <li class="list-inline-item"><a href="#menu"><span></span></a></li>
      </ul>
    </div>
  </div><!-- /.mobile-menu -->
  <nav id="menu" class="stylehome1">
    <ul>
      {{{ output.custom_menu }}}
      {{> theme_edumy/ccn_header_mob_user }}
    </ul>
  </nav>
</div>
